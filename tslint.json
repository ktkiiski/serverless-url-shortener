{
    "extends": "tslint:recommended",
    "rules": {
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "no-bitwise": false,
        "no-require-imports": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false
    },
    "jsRules": {
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never"
                }
            }
        ]
    },
    "rulesDirectory": []
}
